{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w --environment START:serverdev",
    "conf": "rollup -c -w --environment START:serverconf",
    "serverdev": "cd ../server ; npm run dev",
    "serverconf": "cd ../server ; npm run conf",
    "start": "sirv public --single --no-clear --cors",
    "check": "svelte-check --tsconfig ./tsconfig.json"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@tsconfig/svelte": "^2.0.0",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0",
    "svelte-check": "^2.0.0",
    "svelte-preprocess": "^4.0.0",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0"
  },
  "dependencies": {
    "bootstrap": "^5.1.1",
    "js-cookie": "^2.2.1",
    "mediasoup-client": "^3.6.41",
    "page": "^1.11.6",
    "protoo-client": "^4.0.6",
    "random-string": "^0.2.0",
    "redux": "^4.1.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "sirv-cli": "^1.0.0",
    "socket.io-client": "^2.2.0",
    "svelte-redux-connect": "^1.2.1",
    "sveltestrap": "^3.3.2",
    "url-parse": "^1.5.3"
  }
}
